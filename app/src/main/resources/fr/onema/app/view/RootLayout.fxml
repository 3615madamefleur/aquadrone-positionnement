<?xml version="1.0" encoding="UTF-8"?>

<!--<AnchorPane xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.onema.app.view.RootLayoutController">-->

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.76-ea">
    <children>
        <VBox>
            <children>
                <TitledPane fx:id="stateTitledPane" animated="false" onMouseClicked="#resizeParent" text="Etat">
                    <content>
                        <AnchorPane>
                            <children>
                                <HBox maxHeight="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <children>
                                        <VBox>
                                            <children>
                                                <Label text="Etat des connexions">
                                                    <font>
                                                        <Font name="System Bold" size="12.0"/>
                                                    </font>
                                                    <VBox.margin>
                                                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                                                    </VBox.margin>
                                                </Label>
                                                <Separator/>
                                                <Label fx:id="dbLabel" text="Base de données PostGIS">
                                                    <font>
                                                        <Font name="System Bold" size="12.0"/>
                                                    </font>
                                                    <VBox.margin>
                                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                                    </VBox.margin>
                                                </Label>
                                                <Label fx:id="mavlinkLabel" text="Flux MavLink">
                                                    <font>
                                                        <Font name="System Bold" size="12.0"/>
                                                    </font>
                                                    <VBox.margin>
                                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                                    </VBox.margin>
                                                </Label>
                                                <HBox alignment="CENTER" prefHeight="100.0"/>
                                            </children>
                                            <HBox.margin>
                                                <Insets/>
                                            </HBox.margin>
                                        </VBox>
                                        <Separator orientation="VERTICAL" prefWidth="0.0"/>
                                        <VBox>
                                            <children>
                                                <Label text="Précision">
                                                    <font>
                                                        <Font name="System Bold" size="12.0"/>
                                                    </font>
                                                    <VBox.margin>
                                                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                                                    </VBox.margin>
                                                </Label>
                                                <Separator prefWidth="200.0"/>
                                                <HBox alignment="CENTER" layoutX="10.0" layoutY="53.0">
                                                    <children>
                                                        <Label text="Faible">
                                                            <HBox.margin>
                                                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                                                            </HBox.margin>
                                                            <font>
                                                                <Font size="12.0"/>
                                                            </font>
                                                        </Label>
                                                        <ProgressBar fx:id="precisionProgressBar" prefWidth="180.0"
                                                                     progress="0.0">
                                                            <HBox.margin>
                                                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                                                            </HBox.margin>
                                                        </ProgressBar>
                                                        <Label text="Haute">
                                                            <HBox.margin>
                                                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                                                            </HBox.margin>
                                                            <font>
                                                                <Font size="12.0"/>
                                                            </font>
                                                        </Label>
                                                    </children>
                                                </HBox>
                                                <Separator prefWidth="200.0"/>
                                                <Label text="Durée de plongée">
                                                    <font>
                                                        <Font name="System Bold" size="12.0"/>
                                                    </font>
                                                    <VBox.margin>
                                                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                                                    </VBox.margin>
                                                </Label>
                                                <Separator prefWidth="200.0">
                                                    <VBox.margin>
                                                        <Insets/>
                                                    </VBox.margin>
                                                </Separator>
                                                <HBox alignment="CENTER">
                                                    <children>
                                                        <Label text="Courte">
                                                            <HBox.margin>
                                                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                                                            </HBox.margin>
                                                            <font>
                                                                <Font size="12.0"/>
                                                            </font>
                                                        </Label>
                                                        <ProgressBar fx:id="durationProgressBar" prefWidth="180.0"
                                                                     progress="0.0">
                                                            <HBox.margin>
                                                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                                                            </HBox.margin>
                                                        </ProgressBar>
                                                        <Label text="Longue">
                                                            <HBox.margin>
                                                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                                                            </HBox.margin>
                                                            <font>
                                                                <Font size="12.0"/>
                                                            </font>
                                                        </Label>
                                                    </children>
                                                </HBox>
                                            </children>
                                        </VBox>
                                    </children>
                                </HBox>
                            </children>
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                            </padding>
                        </AnchorPane>
                    </content>
                </TitledPane>
                <TitledPane fx:id="sensorsTitledPane" animated="false" onMouseClicked="#resizeParent" text="Capteurs">
                    <content>
                        <AnchorPane prefWidth="300.0">
                            <children>
                                <TableView fx:id="sensorsTableView" maxHeight="150.0" AnchorPane.bottomAnchor="0.0"
                                           AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                           AnchorPane.topAnchor="0.0">
                                    <columns>
                                        <TableColumn fx:id="id" maxWidth="30.0" minWidth="30.0" prefWidth="30.0"
                                                     text="#"/>
                                        <TableColumn fx:id="type" maxWidth="165.0" minWidth="165.0" prefWidth="165.0"
                                                     text="Type"/>
                                        <TableColumn fx:id="state" maxWidth="228.0" minWidth="228.0" prefWidth="228.0"
                                                     text="Etat"/>
                                    </columns>
                                    <columnResizePolicy>
                                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                    </columnResizePolicy>
                                </TableView>
                            </children>
                        </AnchorPane>
                    </content>
                </TitledPane>
                <HBox alignment="CENTER" maxHeight="35.0" minHeight="35.0" prefHeight="35.0">
                    <children>
                        <Button fx:id="configurationButton" maxWidth="160.0" mnemonicParsing="false"
                                onAction="#accessConfiguration" prefWidth="160.0" text="Configuration">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </HBox.margin>
                        </Button>
                        <ProgressIndicator fx:id="progressIndicator">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </HBox.margin>
                        </ProgressIndicator>
                        <Button fx:id="runButton" maxWidth="160.0" mnemonicParsing="false" onAction="#executeMeasures"
                                prefWidth="160.0" text="Démarrer mesures">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
